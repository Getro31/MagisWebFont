{
  "name": "webfontmagis",
  "type": "module",
  "version": "2.1.3",
  "description": "custom web font ",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf solid package *.tgz optimized_svgs",
    "export": "tsx ./node_modules/@figma-export/cli/bin/run use-config figma.config.ts",
    "generate": "node generate-index.js",
    "convert": "npx svgr src/icons --out-dir dist/icons",
    "rollup": "npx rollup -c",
    "css": "node generate-css.js",
    "test": "vitest",
    "build": "run-s clean export generate convert css",
    "np": "np",
    "pack": "run-s test np"
  },
  "author": "Getro",
  "license": "$license",
  "homepage": "https://github.com/Getro31/MagisWebFont#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Getro31/MagisWebFont.git"
  },
  "bugs": {
    "url": "https://github.com/Getro31/MagisWebFont/issues"
  },
  "devDependencies": {
    "@figma-export/cli": "^6.0.2",
    "@figma-export/output-components-as-svg": "^6.0.1",
    "@figma-export/output-styles-as-sass": "^6.0.1",
    "@figma-export/transform-svg-with-svgo": "^6.0.2",
    "@figma-export/types": "^6.0.0",
    "@types/node": "^22.9.0",
    "cheerio": "^1.0.0",
    "dotenv": "^16.4.5",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  },
  "files": [
    "dist",
    "src",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/rollup": "^8.1.0",
    "fontmin": "^1.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "icon-font-generator": "^2.1.11",
    "np": "^10.0.7",
    "path": "^0.12.7",
    "rollup": "^4.27.3",
    "rollup-plugin-svgr": "^2.1.0",
    "svgicons2svgfont": "^15.0.0",
    "svgo": "^3.3.2",
