    "@svgr/cli": "^8.1.0",
    "@svgr/core": "^8.1.0",
    "@svgr/rollup": "^8.1.0",
    "fs": "^0.0.1-security",
    "fs-extra": "^11.2.0",
    "icon-font-generator": "^2.1.11",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "svgo": "^3.3.2"
  },
  "keywords": []
}
